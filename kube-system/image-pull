#/bin/bash
images=(kubernetes-dashboard-amd64:v1.7.0 kubernetes-dashboard-init-amd64:v1.0.0 etcd-amd64:3.0.17 kube-scheduler-amd64:v1.7.6 kube-controller-manager-amd64:v1.7.6 kube-proxy-amd64:v1.7.6 kube-apiserver-amd64:v1.7.6 kube-discovery-amd64:1.0 pause-amd64:3.0 exechealthz-amd64:1.1 k8s-dns-kube-dns-amd64:1.14.4 k8s-dns-sidecar-amd64:1.14.4 k8s-dns-dnsmasq-nanny-amd64:1.14.4 flannel:v0.9.0-amd64)

for imageName in ${images[@]}; do
 docker pull pupudaye/$imageName
 docker tag pupudaye/$imageName gcr.io/google_containers/$imageName
 docker rmi pupudaye/$imageName
done
